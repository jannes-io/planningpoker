{
  "name": "planningpoker",
  "version": "1.0.0",
  "description": "Remote Planning Poker app",
  "scripts": {
    "install-deps": "concurrently \"cd frontend && npm install\" \"cd backend && npm install\"",
    "start-dev": "concurrently \"cd frontend && npm run start\" \"cd backend && npm run watch\"",
    "build": "concurrently \"cd frontend && npm run build\" \"cd backend && npm run build\"",
    "start": "concurrently \"serve -p 80 ./frontend/build/\" \"pm2 start ./backend/dist/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jannes-io/planningpoker.git"
  },
  "author": "Jannes Drijkoningen",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/jannes-io/planningpoker/issues"
  },
  "homepage": "https://github.com/jannes-io/planningpoker#readme",
  "dependencies": {
    "concurrently": "^5.1.0",
    "pm2": "^4.2.3",
    "serve": "^11.3.0"
  }
}
